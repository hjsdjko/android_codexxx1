<?xml version="1.0" encoding="utf-8"?>
<com.design.appproject.widget.MyScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    style="@style/add_box_style"
    android:fillViewport="true">
    <com.design.appproject.widget.MyLinearLayout
        style="@style/add_form_style"
        android:orientation="vertical">
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/yudingbianhao_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="预订编号"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/yudingbianhao_et"
            style="@style/add_item_input_style"
            android:hint="预订编号"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/fangjianhao_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="房间号"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/fangjianhao_et"
            style="@style/add_item_input_style"
            android:hint="房间号"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/kefangleixing_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="客房类型"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/kefangleixing_et"
            style="@style/add_item_input_style"
            android:hint="客房类型"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/jiage_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="价格"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/jiage_et"
            android:inputType="number"
            style="@style/add_item_input_style"
            android:hint="价格"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/ruzhutianshu_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="入住天数"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/ruzhutianshu_et"
            android:inputType="number"
            style="@style/add_item_input_style"
            android:hint="入住天数"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/zongjiage_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="总价格"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/zongjiage_et"
            android:inputType="number"
            style="@style/add_item_input_style"
            android:hint="总价格"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
        <com.design.appproject.widget.MyFlexBoxLayout
            style="@style/add_item_box_style">
            <com.design.appproject.widget.MyTextView
                android:text="预约状态"
                style="@style/add_item_label_style"/>
            <com.design.appproject.widget.BottomSpinner
                android:id="@+id/yuyuezhuangtai_bs"
                style="@style/add_item_picker_box_style"
                android:text="请选择预约状态"/>
        </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="入住日期"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyTextView
            android:id="@+id/ruzhuriqi_tv"
            android:text="请选择入住日期"
            style="@style/add_item_picker_box_style"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="预约时间"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyTextView
            android:id="@+id/yuyueshijian_tv"
            android:text="请选择预约时间"
            style="@style/add_item_picker_box_style"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/yonghuzhanghao_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="用户账号"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/yonghuzhanghao_et"
            style="@style/add_item_input_style"
            android:hint="用户账号"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/yonghuxingming_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="用户姓名"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/yonghuxingming_et"
            style="@style/add_item_input_style"
            android:hint="用户姓名"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/shoujihaoma_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="手机号码"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/shoujihaoma_et"
            style="@style/add_item_input_style"
            android:hint="手机号码"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFlexBoxLayout
        android:id="@+id/louceng_ll"
        style="@style/add_item_box_style">
        <com.design.appproject.widget.MyTextView
            android:text="楼层"
            style="@style/add_item_label_style"/>
        <com.design.appproject.widget.MyEditTextView
            android:id="@+id/louceng_et"
            style="@style/add_item_input_style"
            android:hint="楼层"/>
    </com.design.appproject.widget.MyFlexBoxLayout>
    <com.design.appproject.widget.MyFrameLayout
        style="@style/add_btn_box_style">
        <com.design.appproject.widget.MyButton
            android:id="@+id/submit_btn"
            android:text="提交"
            android:gravity="center"
        style="@style/add_btn_submit_style"/>
    </com.design.appproject.widget.MyFrameLayout>

    </com.design.appproject.widget.MyLinearLayout>
</com.design.appproject.widget.MyScrollView>
